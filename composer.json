{
    "name": "omneo/ap21-sdk",
    "description": "PHP SDK for the Apparel21 API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Omneo\\Apparel21\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omneo\\Apparel21\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/pagination": "^5.4",
        "illuminate/support": "^5.4",
        "nesbot/carbon": "^1.22",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "mockery/mockery": "^0.9.9"
    },
    "config": {
        "sort-packages": true,
        "platform-check": false
    }
}
